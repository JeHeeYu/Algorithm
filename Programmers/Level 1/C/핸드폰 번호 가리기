#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

char* solution(const char* phone_number) {
    char* answer = (char*)malloc(sizeof(char) * strlen(phone_number));
    
    int i;
    
    strcpy(answer, phone_number);
    
    for(i = 0; i < strlen(phone_number) - 4; i++)
    {
        answer[i] = '*';
    }
    
    return answer;
}
