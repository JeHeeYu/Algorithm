#include <stdio.h>
#include <stdbool.h>
#include <stdlib.h>

int check(int number);
int solution(int* lottos, size_t lottos_len, int* win_nums, size_t win_nums_len);


int check(int number)
{
    int rank = 0;
    if ((number == 0) || (number == 1))
        return 6;
    
    rank = 7 - number;
    return rank;
}

int solution(int lottos[], size_t lottos_len, int win_nums[], size_t win_nums_len)
{
    int* answer = (int*)malloc(sizeof(int) * 2);
    
	int yesCount = 0;
	int noCount = 0;
	int i, j;

	for (i = 0; i < lottos_len; i++)
	{
        if(lottos[i] == 0)
        {
            noCount++;
            continue;
        }
            
	    for(j = 0; j < win_nums_len; j++)
        {
            if(lottos[i] == win_nums[j])
            {
                yesCount++;
                break;
            }
                
        }
    }
    answer[0] = check(yesCount + noCount);
    answer[1] = check(yesCount);
    
    return answer;
}
