#include <string>
#include <vector>
#include <iostream>

using namespace std;

string solution(vector<string> seoul) {
    string answer = "김서방은 ";
    int cnt = 0;
    
    for(int i = 0; seoul.size(); i++)
    {
       if(seoul[i] == "Kim")
       {
           cnt = i;
           break;
       }
    }
    
    answer += to_string(cnt);
    answer += "에 있다";
    
    return answer;
}
