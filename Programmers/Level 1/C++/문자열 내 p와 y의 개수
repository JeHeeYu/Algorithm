#include <string>
#include <iostream>
using namespace std;

bool solution(string s)
{
    bool answer = true;
    int cnt = 0;

    for(int i = 0; i < s.size(); i++)
    {
        if(s[i] == 112 || s[i] == 80)
        {
           cnt++;
        }
        if(s[i] == 121 || s[i] == 89)
        {
            cnt--;
        }
    }
    
    if(cnt != 0)
    {
        answer = false;
    }

    return answer;
}
