#include <stdio.h>

void BubbleSort(int* array, int size);
void PrintArray(int* array, int size);

int main()
{
	int array[10] = { 41, 23, 12, 35, 78, 65, 43, 57, 92, 84 };

	BubbleSort(array, 10);
	PrintArray(array, 10);

	return 0;
}

void BubbleSort(int* array, int size)
{
	int i, j, temp;
	for (i = 0; i < n - 1; i++)
	{
		for (j = n - 1; j > i; j--)
		{
			if (a[j - 1] > a[j])
			{
				temp = a[j];
				a[j] = a[j - 1];
				a[j - 1] = temp; 
			}
		}
	}
}

void PrintArray(int* array, int size)
{
	int i;

	for (i = 0; i < size; i++)
	{
		printf("%d ", *(array + i));
	}
}
