#include <stdio.h>

void SelectionSort(int* array, int size);
void PrintArray(int* array, int size);

int main()
{
	int array[10] = { 41, 23, 12, 35, 78, 65, 43, 57, 92, 84 };

	SelectionSort(array, 10);
	PrintArray(array, 10);

	return 0;
}

void SelectionSort(int* array, int size)
{
		int i, j, temp;
		for (i = 0; i < size - 1; i++)
		{
			int min = i;
			for (j = i + 1; j < size; j++)
			{
				if (array[j] < array[min])
				{
					min = j;
				}
				temp = array[i];
				array[i] = array[min];
				array[min] = temp;
			}
		}
	}

void PrintArray(int* array, int size)
{
	int i;

	for (i = 0; i < size; i++)
	{
		printf("%d ", *(array + i));
	}
}
